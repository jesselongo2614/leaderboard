class Players {
    constructor(name,handicap,grossScore,netScore,birdies,thru){
        this.name = name;
        this.handicap = handicap;
        this.grossScore = grossScore;
        this.netScore = netScore;
        this.birdies = birdies;
        this.thru = thru;
    }

    hole1Score(){
        let hole = 1
        let holePar = 4 //Eventually, lets make this reference a program where we input the par and not have it hard coded.
        let holeHandicap = 3 //Eventually, lets make this reference a program where we input the handicap and not have it hard coded.
        this.thru = hole
        var holeEnterGrossScore = parseInt(prompt('Enter '+this.name+'\'s Hole '+hole+' Score: ')) -holePar 
        if(this.handicap>(holeHandicap-1)){
            var holeEnterNetScore = holeEnterGrossScore-1
        }else {
            var holeEnterNetScore = holeEnterGrossScore
        }
        this.grossScore= this.grossScore + holeEnterGrossScore
        this.netScore = this.netScore + holeEnterNetScore
        if(holeEnterGrossScore===-1){
            this.birdies = this.birdies+1
            alert('BIRDIE ALERT')
        }else if(holeEnterGrossScore===-2){
            this.birdies = this.birdies+2
            alert('EAGLE ALERT')
        }
    }
    hole2Score(){
        let hole = 2
