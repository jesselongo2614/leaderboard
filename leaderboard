}











if(numOfGolfers===1){
    document.getElementById("player1Name").textContent = player1.name
    document.getElementById("player1Thru").innerHTML = player1.thru
    document.getElementById("player1GrossScore").innerHTML = player1.grossScore
    document.getElementById("player1NetScore").innerHTML = player1.netScore
    document.getElementById("player1Birdies").innerHTML = player1.birdies
    
}else if(numOfGolfers===2){
    document.getElementById("player1Name").textContent = player1.name
    document.getElementById("player2Name").textContent = player2.name
    document.getElementById("player1Thru").innerHTML = player1.thru
    document.getElementById("player2Thru").innerHTML = player2.thru
    document.getElementById("player1GrossScore").innerHTML = player1.grossScore
    document.getElementById("player2GrossScore").innerHTML = player2.grossScore
    document.getElementById("player1NetScore").innerHTML = player1.netScore
    document.getElementById("player2NetScore").innerHTML = player2.netScore
    document.getElementById("player1Birdies").innerHTML = player1.birdies
    document.getElementById("player2Birdies").innerHTML = player2.birdies

}else if(numOfGolfers===3){

    document.getElementById("player1Name").textContent = player1.name
    document.getElementById("player2Name").textContent = player2.name
    document.getElementById("player3Name").textContent = player3.name
    document.getElementById("player1Thru").innerHTML = player1.thru
    document.getElementById("player2Thru").innerHTML = player2.thru
    document.getElementById("player3Thru").innerHTML = player3.thru
    document.getElementById("player1GrossScore").innerHTML = player1.grossScore
    document.getElementById("player2GrossScore").innerHTML = player2.grossScore
    document.getElementById("player3GrossScore").innerHTML = player3.grossScore
    document.getElementById("player1NetScore").innerHTML = player1.netScore
    document.getElementById("player2NetScore").innerHTML = player2.netScore
    document.getElementById("player3NetScore").innerHTML = player3.netScore
    document.getElementById("player1Birdies").innerHTML = player1.birdies
    document.getElementById("player2Birdies").innerHTML = player2.birdies
    document.getElementById("player3Birdies").innerHTML = player3.birdies


}else if(numOfGolfers===4){
    document.getElementById("player1Name").textContent = player1.name
    document.getElementById("player2Name").textContent = player2.name
    document.getElementById("player3Name").textContent = player3.name
    document.getElementById("player4Name").textContent = player4.name
    document.getElementById("player1Thru").innerHTML = player1.thru
    document.getElementById("player2Thru").innerHTML = player2.thru
    document.getElementById("player3Thru").innerHTML = player3.thru
    document.getElementById("player4Thru").innerHTML = player4.thru
    document.getElementById("player1GrossScore").innerHTML = player1.grossScore
    document.getElementById("player2GrossScore").innerHTML = player2.grossScore
    document.getElementById("player3GrossScore").innerHTML = player3.grossScore
    document.getElementById("player4GrossScore").innerHTML = player4.grossScore
    document.getElementById("player1NetScore").innerHTML = player1.netScore
    document.getElementById("player2NetScore").innerHTML = player2.netScore
    document.getElementById("player3NetScore").innerHTML = player3.netScore
    document.getElementById("player3NetScore").innerHTML = player4.netScore
    document.getElementById("player1Birdies").innerHTML = player1.birdies
    document.getElementById("player2Birdies").innerHTML = player2.birdies
    document.getElementById("player3Birdies").innerHTML = player3.birdies
    document.getElementById("player4Birdies").innerHTML = player4.birdies
}
