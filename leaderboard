var numOfGolfers = parseInt(prompt('Enter Number of Players: '))
var playerList =[]

if(numOfGolfers ===1){
    var player1 = new Players(prompt('Enter Player 1 Name: '),parseInt(prompt('Enter Player 1 Handicap: ')),0,0,0,'Hole ');
    playerList = [player1]
}else if (numOfGolfers ===2){
    var player1 = new Players(prompt('Enter Player 1 Name: '),parseInt(prompt('Enter Player 1 Handicap: ')),0,0,0,'Hole ');
    var player2 = new Players(prompt('Enter Player 2 Name: '),parseInt(prompt('Enter Player 2 Handicap: ')),0,0,0,'Hole ');
    playerList = [player1,player2]
}else if (numOfGolfers ===3){
    var player1 = new Players(prompt('Enter Player 1 Name: '),parseInt(prompt('Enter Player 1 Handicap: ')),0,0,0,'Hole ');
    var player2 = new Players(prompt('Enter Player 2 Name: '),parseInt(prompt('Enter Player 2 Handicap: ')),0,0,0,'Hole ');
    var player3 = new Players(prompt('Enter Player 3 Name: '),parseInt(prompt('Enter Player 3 Handicap: ')),0,0,0,'Hole ');
    playerList = [player1,player2,player3]
}else if(numOfGolfers===4){
    var player1 = new Players(prompt('Enter Player 1 Name: '),parseInt(prompt('Enter Player 1 Handicap: ')),0,0,0,'Hole ');
    var player2 = new Players(prompt('Enter Player 2 Name: '),parseInt(prompt('Enter Player 2 Handicap: ')),0,0,0,'Hole ');
    var player3 = new Players(prompt('Enter Player 3 Name: '),parseInt(prompt('Enter Player 3 Handicap: ')),0,0,0,'Hole ');
    var player4 = new Players(prompt('Enter Player 4 Name: '),parseInt(prompt('Enter Player 4 Handicap: ')),0,0,0,'Hole ');
    playerList = [player1,player2,player3,player4]
}else {
    alert('Not a valid Group Size, please start over')
    window.location.reload()
}



for (var player of playerList){
    player.hole1Score()
}

console.log(player1.grossScore)



